    <file src="..\CenterCLR.RelaxVersioner.Tasks\bin\$Configuration$\netstandard2.0\*.*" target="build/netstandard2.0" />
    <file src="bin\$Configuration$\net461\**\*.*" target="tools/net461" />
    <file src="bin\$Configuration$\netcoreapp2.0\**\*.*" target="tools/netcoreapp2.0" />
    <file src="bin\$Configuration$\netcoreapp2.1\**\*.*" target="tools/netcoreapp2.1" />
    <file src="bin\$Configuration$\netcoreapp2.2\**\*.*" target="tools/netcoreapp2.2" />
    <file src="bin\$Configuration$\netcoreapp3.0\**\*.*" target="tools/netcoreapp3.0" />
    <file src="bin\$Configuration$\netcoreapp2.1\**\*.*" target="tools/netcoreapp3.1" />
    <file src="bin\$Configuration$\net5.0\**\*.*" target="tools/net5.0" />
    <file src="bin\$Configuration$\net6.0\**\*.*" target="tools/net6.0" />
